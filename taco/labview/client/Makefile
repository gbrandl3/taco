TARGET   = TacoSilvia
LM_TARGET = lv_menu

first: all
####### Implicit rules






.SUFFIXES: .c .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules





all: $(TARGET) lv_menu

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

$(LM_TARGET): $(LM_OBJECTS)
	$(LINK) -o $(LM_TARGET) $(LM_OBJECTS) $(LIBS) 
 
mocables: $(SRCMOC)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

dist: 
	@mkdir -p .tmp/TacoSilvia && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/TacoSilvia/ && ( cd `dirname .tmp/TacoSilvia` && $(TAR) TacoSilvia.tar TacoSilvia && $(GZIP) TacoSilvia.tar ) && $(MOVE) `dirname .tmp/TacoSilvia`/TacoSilvia.tar.gz . && $(DEL_FILE) -r .tmp/TacoSilvia

mocclean:
	-$(DEL_FILE) $(OBJMOC)
	-$(DEL_FILE) $(SRCMOC)

uiclean:

yaccclean:
lexclean:
clean: mocclean
	-$(DEL_FILE) $(OBJECTS) 
	-$(DEL_FILE) *~ core *.core


####### Sub-libraries

distclean: clean
	-$(DEL_FILE) $(TARGET) $(TARGET)


FORCE:

####### Compile


lv_menu.o: lv_menu.cpp LabviewClient.h

main.o: main.cpp TacoSilvia.h 

TacoSilvia.o: TacoSilvia.cpp TacoSilvia.h LabviewClient.h Pixmaps.h

LabviewClient.o: LabviewClient.cpp LabviewClient.h


moc_TacoSilvia.o: moc_TacoSilvia.cpp TacoSilvia.h 

moc_TacoSilvia.cpp: $(MOC) TacoSilvia.h
	$(MOC) TacoSilvia.h -o moc_TacoSilvia.cpp

####### Install

install: all 

