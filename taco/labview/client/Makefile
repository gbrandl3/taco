####### Compiler, tools and options

CC       = gcc
CXX      = g++
LEX      = flex
YACC     = yacc

DS_HOME = $(DSHOME)
LIB_HOME = $(DS_HOME)/lib/linux/x86
LV_HOME = /usr/local/lv61/
LV_TACO_HOME = ../
LIB_LVTACO = $(LV_TACO_HOME)/server/lib/
LIB_LV_INCLUDE = $(LV_TACO_HOME)/server/include/
TACO_LV_DIR=$(LV_TACO_HOME)/client/
LV_DEBUG = -DLV_DEBUG
LV_DC = -DLV_DC


CFLAGS   = -pipe -O2 -march=i586 -mcpu=i686 -fmessage-length=0 -fPIC -DNO_DEBUG -Wall -W -O2 -march=i586 -mcpu=i686 -fmessage-length=0 -fPIC -DNO_DEBUG  -DQT_NO_DEBUG $(LV_DEBUG) $(LV_DC) -DLABVIEW -DNOMAIN
CXXFLAGS = -pipe -O2 -march=i586 -mcpu=i686 -fmessage-length=0 -fPIC -DNO_DEBUG -Wall -W -O2 -march=i586 -mcpu=i686 -fmessage-length=0 -fPIC -DNO_DEBUG  -DQT_NO_DEBUG $(LV_DEBUG) $(LV_DC) -DLABVIEW -DNOMAIN
LEXFLAGS = 
YACCFLAGS= -d
INCPATH  = 	-I$(QTDIR)/mkspecs/linux-g++ \
				-I. \
				-I/usr/include \
				-I$(QTDIR)/include \
				-I$(DS_HOME)/include -g \
				-I$(DS_HOME)/include/private \
				-I$(LV_HOME)/cintools \
				-I$(TACO_LV_DIR) \
				-I$(LIB_LV_INCLUDE) 

LINK     = g++
LFLAGS   = 
LIBS     = $(SUBLIBS) -L/usr/lib/ -L$(QTDIR)/lib/ -L/usr/X11R6/lib/ -L$(LIB_LVTACO) -ltaco -lqt -lXext -lX11 -lm
AR       = ar cqs
RANLIB   = 
MOC      = $(QTDIR)/bin/moc
UIC      = $(QTDIR)/bin/uic
QMAKE    = qmake
TAR      = tar -cf
GZIP     = gzip -9f
COPY     = cp -f
COPY_FILE= $(COPY) -p
COPY_DIR = $(COPY) -pR
DEL_FILE = rm -f
SYMLINK  = ln -sf
DEL_DIR  = rmdir
MOVE     = mv -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p

####### Output directory

OBJECTS_DIR = ./

####### Files

HEADERS = TacoSilvia.h \
			 LabviewClient.h \
			 Pixmaps.h

SOURCES = main.cpp \
			 TacoSilvia.cpp \
			 LabviewClient.cpp \

OBJECTS = main.o \
			 TacoSilvia.o \
			 LabviewClient.o 


