#!/bin/csh

setenv DBTABLES_STANDARD NAMES,PS_NAMES,CLASS,SYS,ERROR,CMDS,SEC,EVENTS
# add your DOAMIN tables here
setenv  DBTABLES_DOMAIN TEST
setenv DBTABLES ${DBTABLES_STANDARD},${DBTABLES_DOMAIN}


setenv DSHOME @prefix@
setenv RES_BASE_DIR ${DSHOME}/dbase/res
setenv DBM_DIR ${DSHOME}/dbase/dbm

if (${?LD_LIBRARY_PATH}) then
setenv LD_LIBRARY_PATH ${LD_LIBRARY_PATH}:${DSHOME}/lib/@taco_install_path@
else
setenv LD_LIBRARY_PATH ${DSHOME}/lib/@taco_install_path@
endif

if (${?PYTHONPATH}) then
setenv PYTHONPATH ${PYTHONPATH}:${DSHOME}/lib/@taco_install_path@:$DSHOME/python
else
setenv PYTHONPATH ${DSHOME}/lib/@taco_install_path@$DSHOME/python
endif
if (${?NETHOST}) then
	;
# echo "Nethost= " $NETHOST
else
setenv NETHOST localhost
endif
setenv PATH ${PATH}:${DSHOME}/system/bin/@taco_install_path@
setenv PATH ${PATH}:${DSHOME}/bin/@taco_install_path@
