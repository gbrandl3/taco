#! /bin/sh

prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
bindir=@bindir@
sbindir=@sbindir@
datadir=@datadir@
pythondir=@pythondir@
pyexecdir=@pyexecdir@

# Add your domain tables here (comma separated)
DBTABLES_CUSTOM=${DBTABLES_CUSTOM:-TEST}

# if you want to start the TACO services in secure mode set "yes" else "no"
TACO_SECURE_MODE=no

# if you want to start the TACO services with logging set "yes" else "no"
TACO_LOGGING=yes

# set the database type "dbm" or "mysql"
TACO_DATABASE=dbm

# It is unlikely that the rest of the file needs a modification
export DSHOME=${DSHOME:-${prefix}}
export DBTABLES=NAMES,PS_NAMES,CLASS,SYS,ERROR,CMDS,SEC,EVENTS,${DBTABLES_CUSTOM}
export RES_BASE_DIR=${RES_BASE_DIR:-@resdatadir@}
export DBM_DIR=@dbmdir@
export LOGPATH=${DSHOME}/log
export NETHOST=${NETHOST:-`hostname|cut -f1 -d'.'`}
if [ "$DSHOME" = "$prefix" ] ; then
	export LD_LIBRARY_PATH=@libdir@${LD_LIBRARY_PATH:+:}${LD_LIBRARY_PATH}
	export TACO_PATH=@sbindir@
	export PATH=${TACO_PATH}:@bindir@${PATH:+:}${PATH}
else
	export LD_LIBRARY_PATH=${DSHOME}/lib/${LD_LIBRARY_PATH:+:}${LD_LIBRARY_PATH}
	export TACO_PATH=${DSHOME}/sbin/
	export PATH=${TACO_PATH}:${DSHOME}/bin/${PATH:+:}${PATH}
fi

export PYTHONPATH=@pkgpythondir@:@pkgpyexecdir@${PYTHONPATH:+:}${PYTHONPATH}
unset DBTABLES_CUSTOM

export MYSQL_USER=root
export MYSQL_PASSWORD=
